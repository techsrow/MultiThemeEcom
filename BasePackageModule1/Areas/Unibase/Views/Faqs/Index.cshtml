@model IEnumerable<BasePackageModule1.Models.Faq>

@{
    ViewData["Title"] = "Index";
}
<p>
    <a class="btn btn-primary" asp-action="Create">Create New</a>
</p>
<table id="simpletable" class="table table-striped table-bordered nowrap" style="font-size:13px;">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FaqQuestion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FaqAns)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FaqQuestion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FaqAns)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
