@model BasePackageModule2.Models.AboutUs

@{
    ViewData["Title"] = "Details";
}
    <div class="col-sm-12">
        <div class="card">
           
            <div class="card-block">
                <div class="row">
                    <div class="col-12">
                        <h1>Details</h1>

                        <div>
                            <h4>AboutUs</h4>
                            <hr />
                            <dl class="row">
                                <dt class="col-sm-2">
                                    Image
                                </dt>
                                <dd class="col-sm-10">
                                    <img src="@Url.Content(Model.Image)" class="img-fluid" />
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.Title)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Title)
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.Description)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.Raw(Model.Description);
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.Keywords)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Keywords)
                                </dd>

                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.Video)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Video)
                                </dd>
                            </dl>
                        </div>
                        <div>
                            <a class="btn btn-success" asp-action="Edit" asp-route-id="@Model.AboutUsId">Edit</a>
                            <a class="btn  btn-primary" asp-action="Index">Back to List</a>
                        </div>
                    </div>
</div>
</div>
</div>
</div>
