                                        <input asp-for="billing_city" class="form-control" type="text" data-val="true" data-val-required="The Billing City field is required." id="billing_city" name="billing_city" />
                                        <span asp-validation-for="billing_city" class="text-danger field-validation-valid" data-valmsg-for="billing_city" data-valmsg-replace="true"></span>
