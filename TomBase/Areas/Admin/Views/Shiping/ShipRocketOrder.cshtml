<div id="kt_content_container" class="container-xxl">
    <div class="row-cols-1">
        <div class="card-bordered">
            @if (ViewBag.Shiprocketerror != "")
            {
                <div class="alert alert-danger  alert-dismissible fade show" role="alert">
                    <strong>@ViewBag.Shiprocketerror</strong>

                </div>
            }

        <form asp-action="ShipRocketOrder" id="Myform" style="width:100%;">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="table-responsive">
                <table class="table table-danger table-hover p-4">
                    <thead style="padding-left:10px;">
                        <tr>
                            <th style="padding-left:10px;">Name</th>
                            <th>sku</th>
                            <th>Units</th>
                            <th>Discount</th>
                            <th>Selling Price</th>

                        </tr>
                    </thead>
                    <tbody>

                        @for (int i = 0; i < Model.order_items.Count; i++)
                        {


                            <tr>
                                <td style="padding-left:10px;">@Model.order_items[i].name <input asp-for="@Model.order_items[i].name" class="form-control" hidden /></td>
                                <td>@Model.order_items[i].sku <input asp-for="@Model.order_items[i].sku " class="form-control" hidden /></td>
                                <td>@Model.order_items[i].units <input asp-for="@Model.order_items[i].units" class="form-control" hidden /></td>
                                <td>@Model.order_items[i].discount <input asp-for="@Model.order_items[i].discount" class="form-control" hidden /></td>
                                <td>@Model.order_items[i].selling_price <input asp-for="@Model.order_items[i].selling_price" class="form-control" hidden /></td>

                            </tr>
                        }

                    </tbody>
                </table>

                <div class="form-group  col-md-6">
                    <label asp-for="order_id" class="control-label" hidden></label>
                    <input asp-for="order_id" class="form-control" hidden />
                    <input asp-for="order_id" class="form-control" disabled />
                    <span asp-validation-for="order_id" class="text-danger"></span>
                </div>
            </div>

            <div class="accordion" id="kt_accordion_1">
                <div class="accordion-item">
                    <h2 class="accordion-header" id="kt_accordion_1_header_1">
                        <button class="accordion-button fs-4 fw-semibold" type="button" data-bs-toggle="collapse" data-bs-target="#kt_accordion_1_body_1" aria-expanded="true" aria-controls="kt_accordion_1_body_1">
                            Buyers Details #1
                        </button>
                    </h2>
                    <div id="kt_accordion_1_body_1" class="accordion-collapse collapse show" aria-labelledby="kt_accordion_1_header_1" data-bs-parent="#kt_accordion_1">
                        <div class="accordion-body">
                            <div class="form-group  col-md-6">
                                <label class="control-label" asp-for="order_id" hidden for="order_id">Order ID</label>
                                <input class="form-control" asp-for="order_id" hidden type="text" data-val="true" data-val-required="The Order ID field is required." id="order_id" name="order_id" />
                                <span class="text-danger field-validation-valid" asp-validation-for="order_id" data-valmsg-for="order_id" data-valmsg-replace="true"></span>
                            </div>
                            <div class="form-group col-md-6">
                                <label class="control-label" hidden for="order_date" asp-for="order_date">Order Date</label>
                                <input class="form-control" type="datetime-local" asp-for="order_date" hidden data-val="true" data-val-required="The Order Date field is required." id="order_date" name="order_date" />

                                <span class="text-danger field-validation-valid" asp-validation-for="order_date" data-valmsg-for="order_date" data-valmsg-replace="true"></span>
                            </div>
                            @*Content Here*@
                            <div class="row">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label class="control-label"> Buyer Name</label>
                                        <input class="form-control" type="text" data-val="true" data-val-required="The Billing Customer Name  field is required." id="billing_customer_name" name="billing_customer_name" asp-for="billing_customer_name" />
                                        <span class="text-danger field-validation-valid" asp-validation-for="billing_customer_name" data-valmsg-for="billing_customer_name" data-valmsg-replace="true"></span>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label class="control-label"> Last Name</label>
                                        <input class="form-control" type="text" id="billing_last_name" name="billing_last_name" asp-for="billing_last_name" />
                                        <span class="text-danger field-validation-valid" asp-validation-for="billing_last_name" data-valmsg-for="billing_last_name" data-valmsg-replace="true"></span>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label class="control-label"> Email</label>
                                        <input asp-for="billing_email" class="form-control" type="email" data-val="true" data-val-email="Invalid Email Address" data-val-required="The Billing Email field is required." id="billing_email" name="billing_email" />
                                        <span asp-validation-for="billing_email" class="text-danger field-validation-valid" data-valmsg-for="billing_email" data-valmsg-replace="true"></span>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label class="control-label"> Phone</label>
                                        <input asp-for="billing_phone" class="form-control" type="text" data-val="true" data-val-required="The Billing Phone field is required." id="billing_phone" name="billing_phone" />
                                        <span asp-validation-for="billing_phone" class="text-danger field-validation-valid" data-valmsg-for="billing_phone" data-valmsg-replace="true"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-6">
                                    <div class="form-group">
                                        <label class="control-label"> Alternet Phone</label>
                                        <input asp-for="billing_alternate_phone" class="form-control" type="number" data-val="true" data-val-required="The Billing Alternate Phone field is required." id="billing_alternate_phone" name="billing_alternate_phone" />
                                        <span asp-validation-for="billing_alternate_phone" class="text-danger field-validation-valid" data-valmsg-for="billing_alternate_phone" data-valmsg-replace="true"></span>
                                    </div>
                                </div>
                            </div>


                            <hr />



                        </div>
                    </div>
                </div>

