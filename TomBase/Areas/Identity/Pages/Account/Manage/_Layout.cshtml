<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @await Component.InvokeAsync("Favicon")
    @await Component.InvokeAsync("Seo")
    @await RenderSectionAsync("MetaTags", required: false)
    <link rel="stylesheet" type="text/css" href="~/theme/css/font-awesome.min.css" />
    <link rel="stylesheet" type="text/css" href="~/theme/css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/theme/css/jquery-ui.css">
    <link rel="stylesheet" type="text/css" href="~/theme/css/owl.carousel.css">
    <link rel="stylesheet" type="text/css" href="~/theme/css/fotorama.css">
    <link rel="stylesheet" type="text/css" href="~/theme/css/magnific-popup.css">
    <link rel="stylesheet" type="text/css" href="~/theme/css/custom.css">
    <link rel="stylesheet" type="text/css" href="~/theme/css/responsive.css">

    <link rel="apple-touch-icon" href="images/apple-touch-icon.html">
    <link rel="apple-touch-icon" sizes="72x72" href="images/apple-touch-icon-72x72.html">
    <link rel="apple-touch-icon" sizes="114x114" href="images/apple-touch-icon-114x114.html">

    <style>
        #header {
            background-color: #54D3FA;
            padding: 25px;
        }
    </style>

</head>
<body class="homepage">
    <div class="se-pre-con"></div>
  
    
    <div class="main">
        @await Component.InvokeAsync("Navigation")
        @await RenderSectionAsync("Header", required: false);
        <div class="container" style="margin-top:60px; margin-bottom:100px;">

            <div class="row">
                <div class="col-md-3">
                    <partial name="_ManageNav" />
                </div>
                <div class="col-md-9">
                    @RenderBody()
                </div>
            </div>
        </div>

        
        @await Component.InvokeAsync("Footer")

        @await Component.InvokeAsync("ChatBot")


        @RenderSection("Scripts", required: false)


    </div>
    <script src="~/theme/js/jquery-1.12.3.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js"></script>
    <script src="~/theme/js/bootstrap.min.js"></script>
    <script src="~/theme/js/jquery.downCount.js"></script>
    <script src="~/theme/js/jquery-ui.min.js"></script>
    <script src="~/theme/js/fotorama.js"></script>
    <script src="~/theme/js/jquery.magnific-popup.js"></script>
    <script src="~/theme/js/owl.carousel.min.js"></script>
    <script src="~/theme/js/custom.js"></script>

    
</body>
</html>